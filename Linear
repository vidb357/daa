#include <stdio.h>

void LS(int a[], int find, int len){

    int i, flag =0 ;
    for(i=0; i<len; i++){

        if(a[i]==find){
            flag=1;
            printf("Found at %d\n", i+1);
            
        }
    }

    if (flag==0){
        printf("not found");
    }

    return;
}

int main(){

    int a[5]={8,2,3,1,1};
    LS(a,1,5);
    return 0;
}
